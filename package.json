{
  "name": "@cylution/is-cloudflare-ip",
  "version": "1.0.0-0",
  "description": "check if request IP come from Cloudflare",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "prepack": "tsc"
  },
  "keywords": [
    "check ip",
    "ip",
    "cloudflare"
  ],
  "author": "0x0a0d",
  "license": "MIT",
  "devDependencies": {
    "@cylution/eslint-config-nodejs": "^1.0.7-alpha.0",
    "@types/ip": "^1.1.0",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/parser": "^4.30.0",
    "jest": "^27.1.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "ip": "^1.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cylution-org/is-cloudflare-ip"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "coverageDirectory": "<rootDir>/coverage",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.(ts|js)"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    }
